Private Sub cmb_sy_KeyUp(KeyCode As Integer, Shift As Integer)
     MsgBox "Please select a school year from the list."
    cmb_sy.Text = ""
End Sub

Private Sub Form_Load()
     Call set_datagrid(dg_average, rs_average, "SELECT * From (SELECT a.SY, a.ID,a.section_name,a.subject_code, GROUP_CONCAT(b.grade SEPARATOR ',') as all_grades, SUM(b.grade) / 4 From (SELECT * From tbl_grade WHERE ID='" & lbl_id.Caption & "') a Left Join tbl_grade b ON a.SY = b.SY AND a.id = b.id AND a.section_name = b.section_name) tbl_temp WHERE section_name = '" & lbl_section.Caption & "'AND ID='" & lbl_id.Caption & "'")
End Sub
